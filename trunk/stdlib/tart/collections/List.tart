/** List interface */
interface List[%ElementType] : Iterable[ElementType] {
  
  /** Append an item to the end of the list. */
  def add(e:ElementType);

  /** Access a list element by index. */
  def [index:int]:ElementType { get; set; }
  
  /** The number of items in the list. */
  def length:int { get; }
}
