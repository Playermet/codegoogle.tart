/** Map interface */
interface Map[%Key, %Value] : Iterable[(Key, Value)] {

  /** Access a value by key. */
  def [key:Key]:Value { get; set; }
  
  /** The number of items in the map. */
  def length:int { get; }

  /** Return true if there are no entries in this map. */
  def isEmpty:bool { get; }
}
