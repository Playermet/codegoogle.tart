/** Attribute that indicates that the associated function is a
    built-in compiler intrinsic. */
//@Attribute(Attribute.Target.FUNCTION | Attribute.Target.MACRO)
@Attribute(Attribute.Target.CALLABLE)
class Intrinsic {
  private var tag:String;
  public def construct() { self.tag = ""; }
  public def construct(tag:String) { self.tag = tag; }
}
