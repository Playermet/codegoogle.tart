import tartx.lexgen.shared.Location;

class MatchCharactersNode : Node {
  private var _range:CharacterRange;
  
  def construct(location:Location, range:CharacterRange) {
    super(NodeType.MATCH_CHARACTERS, location);
    self._range = range;
  }
  
  def range:CharacterRange { get { return _range; } }
}
