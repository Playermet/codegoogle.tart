[EntryPoint]
def main(args: String[]) {
  // Check numeric limits.
  Debug.assertEq(1, Math.min(1, 2));
  Debug.assertEq(1, Math.min(1, 2, 3));
  Debug.assertEq(1, Math.min(1, 2, 3, 4));
  Debug.assertEq(2, Math.max(1, 2));
  Debug.assertEq(3, Math.max(1, 2, 3));
  Debug.assertEq(4, Math.max(1, 2, 3, 4));

  // TODO: sin, cos, sqrt tests


  return 0;
}
