@EntryPoint
def main(args:String[]) -> int {
  Debug.assertEq("One", switchTest(1));
  Debug.assertEq("Two", switchTest(2));
  Debug.assertEq("Three", switchTest(3));
  
  return 0;
}

def switchTest(value:int) -> String {
  switch value {
    case 1 { return "One"; }
    case 2 { return "Two"; }
    case 3 { return "Three"; }
  }
  
  return "Fail";
}
