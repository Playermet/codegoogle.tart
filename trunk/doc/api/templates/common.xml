<?xml version="1.0" encoding="UTF-8"?>
<html xmlns:py="http://genshi.edgewall.org/"
      xmlns:xi="http://www.w3.org/2001/XInclude"
      py:strip="True">

<xi:include href="${template}" py:for="template in gen.custom"/>

<link href='http://fonts.googleapis.com/css?family=Ubuntu:regular,italic'
      rel='stylesheet' type='text/css'/>
<link rel="stylesheet" type="text/css" href="apidocs.css"/>
<meta charset="UTF-8"/>

<py:def function="stylesheet">
</py:def>

<py:def function="nav_item(text, url, selected)">
	<nav py:if="selected" class="nav-item nav-item-selected">${text}</nav>
  <nav py:if="not selected" class="nav-item">
    <a class="nav-link" href="${url}">${text}</a>
  </nav>
</py:def>

<py:def function="page_header(current)">
  <section class="page-header">
    <header id="header-title" class="page-header-title">Page Title</header>
  </section>
  <section class="nav-section">
    ${nav_item("Packages", "index.html", current == "packages")}
    &#8226;
    ${nav_item("Types", "types.html", current == "types")}
    &#8226;
    ${nav_item("Hierarchy", "hierarchy.html", current == "hierarchy")}
    &#8226;
    ${nav_item("Namespaces", "namespaces.html", current == "namespaces")}
    &#8226;
    ${nav_item("Index", "names.html", current == "names")}
  </section>
</py:def>

<py:def function="page_footer">
  <section class="page-footer">Generated by doclobster.</section>
</py:def>

</html>
