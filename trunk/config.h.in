/* ================================================================== *
 * Compiler Configuration
 * ================================================================== */

#cmakedefine HAVE_NEW 1
#cmakedefine HAVE_CTIME 1
#cmakedefine HAVE_CLOCK_IN_STD 1
#cmakedefine HAVE_STDIO_H 1
#cmakedefine HAVE_STDINT_H 1
#cmakedefine HAVE_STDDEF_H 1
#cmakedefine HAVE_STDLIB_H 1
#cmakedefine HAVE_STRING_H 1
#cmakedefine HAVE_STDBOOL_H 1
#cmakedefine HAVE_STDARG_H 1
#cmakedefine HAVE_MALLOC_H 1
#cmakedefine HAVE_UNISTD_H 1
#cmakedefine HAVE_FCNTL_H 1
#cmakedefine HAVE_INTTYPES_H 1
#cmakedefine HAVE_ASSERT_H 1
#cmakedefine HAVE_ERRNO_H 1
#cmakedefine HAVE_EXECINFO_H 1
#cmakedefine HAVE_SYS_STAT_H 1
#cmakedefine HAVE_SYS_TIME_H 1
#cmakedefine HAVE_SYS_TYPES_H 1
#cmakedefine HAVE_SYS_RESOURCE_H 1
#cmakedefine HAVE_LIBKERN_OSATOMIC_H 1
#cmakedefine HAVE_CXXABI_H 1
#cmakedefine HAVE_DLFCN_H 1
#cmakedefine HAVE_UNWIND_H 1
#cmakedefine HAVE_LIBUNWIND_H 1

/** Whether the backtrace() function is available. */
#cmakedefine HAVE_BACKTRACE 1

/** Whether the dladdr() function is available. */
#cmakedefine HAVE_DLADDR 1

/** Whether GCC builtin atomic functions are available. */
#cmakedefine HAVE_GCC_ATOMICS 1

/** Whether GCC thread local support is available. */
#cmakedefine HAVE_GCC_THREAD_LOCAL 1

/** Whether MSVC thread local support is available. */
#cmakedefine HAVE_MSVC_THREAD_LOCAL 1

/** Whether the posix_memalign() function is available. */
#cmakedefine HAVE_POSIX_MEMALIGN 1

/** Whether the _aligned_malloc function is available. */
#cmakedefine HAVE_ALIGNED_MALLOC 1

/** Whether the valloc() function is available. */
#cmakedefine HAVE_VALLOC 1

/** Whether pthreads is available. */
#cmakedefine HAVE_PTHREADS 1

/** Whether the stat() function is available. */
#cmakedefine HAVE_STAT 1

#if _MSC_VER
  #define snprintf _snprintf
#endif

/** Type sizes */
#cmakedefine SIZEOF_INT @SIZEOF_INT@
#cmakedefine SIZEOF_LONG @SIZEOF_LONG@
#cmakedefine SIZEOF_LONG_LONG @SIZEOF_LONG_LONG@
#cmakedefine SIZEOF_SIZE_T @SIZEOF_SIZE_T@
#cmakedefine SIZEOF_VOID_PTR @SIZEOF_VOID_PTR@

