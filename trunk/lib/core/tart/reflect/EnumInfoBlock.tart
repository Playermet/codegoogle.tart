import tart.core.Memory.Address;

/** Structure that is used to hold the compressed reflection information
    for an enum type. */
@Reflection(Reflection.Detail.NONE) struct EnumInfoBlock {
  /** Constants pool for this enum. */
  private var _meta:ReflectionMetadata;
  
  /** Encoded definition of this enum type. */
	private var _encodedDefn:Address[ubyte];
	
	/** Lazily-initialized type object. */
	private var _type:optional EnumType;

	//def type:EnumType { get {
	//} }
}
