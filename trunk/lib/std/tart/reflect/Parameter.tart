/** Reflection class for a method parameter. This structure is shared by all parameters
    that have the same name, unless the list of attributes is non-empty.
  */
class Parameter {
  private {
    /** The unqualified member name. */
    var _name:String;
  
    /** Array of attributes for this parameter. */
    var _attributes:Attribute[];
  }

  private def construct(name:String) {
    self._name = name;
  }
}
